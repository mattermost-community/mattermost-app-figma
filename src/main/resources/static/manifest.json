{
  "app_id": "spring-boot-example",
  "version": "0.7.0",
  "icon": "icon.png",
  "display_name": "I'm an App!",
  "homepage_url": "https://github.com/mattermost/mattermost-plugin-apps/dev",
  "requested_permissions": [
    "act_as_bot",
    "remote_oauth2",
    "act_as_user"
  ],
  "requested_locations": [
    "/command"
  ],
  "bindings": {
    "path": "/bindings",
    "expand": {
      "acting_user": "all",
      "oauth2_app": "all",
      "oauth2_user": "all"
    }
  },
  "get_oauth2_connect_url": {
    "path": "/oauth2/connect",
    "expand": {
      "acting_user": "all",
      "oauth2_app": "all",
      "oauth2_user": "all"
    }
  },
  "on_oauth2_complete": {
    "path": "/oauth2/complete",
    "expand": {
      "oauth2_app": "all",
      "acting_user_access_token": "all"
    }
  },

  "http":{"root_url":"http://localhost:8080"},
  "assets": []
}